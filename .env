# Database
DATABASE_URL=sqlite+aiosqlite:///./timelink.db

# Security
SECRET_KEY=your-super-secret-key-change-this-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS
BACKEND_CORS_ORIGINS=["http://localhost:3000", "https://timelink.digital"]

# Environment
ENVIRONMENT=development
DEBUG=true

# Suno AI API (실제 사용시)
SUNO_API_KEY=your-suno-api-key
SUNO_API_BASE_URL=https://api.suno.ai/v1

# TL Token Settings
TL3_INITIAL_BALANCE=1000
TL_RELEASE_RATE=10
CAR_MODE_MULTIPLIER=2.0
